{
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.spec.ts",
    "dist/*",
    "coverage/*",
    "playground/*",
    "test*",
    "src/**/index.ts",
    "index.ts"
  ],
  "reporter": [
    "html",
    "lcov"
  ],
  "statements": 95,
  "branches": 95,
  "functions": 95,
  "lines": 95,
  "check-coverage": true,
  "all": true
}
